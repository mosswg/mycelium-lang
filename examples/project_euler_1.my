func main {
    int sum = 0
    // Add all the multiples of 3 from 3 to 999 to sum
    int i = 3
    while (i < 1000) {
        sum += i
        i += 3
    }

    // Add all the multiples of 5 from 5 to 999 to sum
    i = 5
    while (i < 1000) {
    // Ignore multiples of three since we've already added them to sum
        if (i % 3 != 0) {
            sum += i
        }
        i += 5
    }
    println(sum);
}
